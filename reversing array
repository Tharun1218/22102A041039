import java.util.*;
class Main{
  public static void main(String args[]){
    Scanner sc=new Scanner (System.in);
    System.out.print("enter the size of the array: ");
    int n=sc.nextInt();
    int[] arr=new int[n];
    System.out.print("enter array elements: ");
    
    for (int i=0;i<n;i++){
        arr[i]=sc.nextInt();
    }
    for(int i=n-1;i>=0;i--){
     System.out.print(arr[i]+" ");
    }
  }
}
